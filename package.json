    "body-parser": "^1.19.0",
    "cls-hooked": "^4.2.2",
    "comparisons": "3.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.8.0",
    "flat": "^5.0.2",
    "glob": "^7.2.0",
    "i18next": "^21.5.6",
    "i18next-http-middleware": "^3.1.4",
    "jest-environment-node": "^27.4.2",
    "lodash": "^4.17.20",
    "m2e-validator": "^1.3.9",
    "micromatch": "^4.0.2",
    "moment": "^2.29.3",
