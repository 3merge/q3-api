  "engines": {},
  "scripts": {
    "postinstall": "yarnw-sync-deps && yarn-deduplicate",
    "bootstrap": "lerna bootstrap --use-workspaces",
    "lint": "eslint packages --ext .js",
    "test:packages-wordcount": "jest q3-plugin-wordcount --detectOpenHandles --forceExit",
    "test:packages-exports": "jest q3-api-exports --detectOpenHandles --forceExit",
    "test:packages-sequence": "jest q3-plugin-sequence --detectOpenHandles --forceExit",
    "test:packages-ngrams": "jest q3-plugin-ngrams --detectOpenHandles --forceExit",
    "test:packages-extref": "jest q3-plugin-extref --detectOpenHandles --forceExit",
    "test:packages-scheduler": "jest q3-core-scheduler --detectOpenHandles --forceExit",
    "test:packages-schedulerdispatch": "jest q3-plugin-schedulerdispatch --detectOpenHandles --forceExit",
    "test:packages-changestream": "jest q3-plugin-changelog --detectOpenHandles --forceExit",
    "test:packages": "jest \"^(.*?)\\.unit.test.js\"  --runInBand  --detectOpenHandles --forceExit --collectCoverage --coverageDirectory coverage/coverage_units",
    "test:integrations": "jest \"^(.*?)\\.int.test.js\" --runInBand --detectOpenHandles --forceExit --collectCoverage --coverageDirectory coverage/coverage_integrations",
    "test:e2e": "yarn workspace e2e-tests test",
    "jest": "jest",
    "test": "yarn test:packages && yarn test:packages-scheduler && yarn test:packages-schedulerdispatch && yarn test:packages-ngrams && yarn test:packages-extref && yarn test:packages-sequence && yarn test:packages-changestream && yarn test:packages-exports && yarn test:e2e",
    "upload": "lerna publish",
    "demo-web": "nodemon demo",
    "demo-worker": "nodemon demo/worker",
    "bin:build-access": "yarn build-access",
    "bin:build-locale": "yarn build-locale",
    "bin:seed-emails": "yarn seed-emails",
    "bin:seed-login": "yarn seed-login",
    "version-workspace": "lerna version"
  },
  "workspaces": [
    "demo",
    "e2e-tests",
    "packages/*"
  ],
  "devDependencies": {
    "eslint": "^7.15.0",
    "eslint-config-node-3merge": "^1.2.1",
    "jest": "^27.4.3",
    "jest-watch-typeahead": "^1.0.0",
    "jest-watch-yarn-workspaces": "^1.1.0",
    "lerna": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "yarn-deduplicate": "^3.1.0",
